{
  "bin": {
    "ecommerce": "bin/ecommerce.js"
  },
  "description": "An e-commerce project used to explore event-based APIs and microservices.",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.620.0",
    "@aws-sdk/client-s3": "3.620.0",
    "@aws-sdk/lib-dynamodb": "3.620.0",
    "@effect/schema": "0.69.2",
    "@sindresorhus/is": "7.0.0",
    "aws-cdk-lib": "2.150.0",
    "constructs": "10.3.0",
    "effect": "3.5.8",
    "nanoid": "5.0.7",
    "parse-json": "8.1.0",
    "pkg-dir": "8.0.0",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@effect/vitest": "0.6.12",
    "@faker-js/faker": "8.4.1",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/aws-lambda": "8.10.142",
    "@types/node": "20.14.12",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "@vitest/coverage-v8": "1.6.0",
    "aws-cdk": "2.150.0",
    "del-cli": "5.1.0",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "fishery": "2.2.2",
    "husky": "9.1.3",
    "lint-staged": "15.2.7",
    "pactum": "3.7.0",
    "prettier": "3.3.3",
    "release-it": "17.6.0",
    "testdouble": "3.20.2",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "vitest": "1.6.0"
  },
  "name": "ecommerce",
  "scripts": {
    "build": "tsc",
    "build:ci": "npm run clean && npm test && tsc",
    "cdk": "cdk",
    "clean": "del-cli coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky",
    "release": "release-it --ci",
    "test": "vitest run --dir test/unit",
    "test:cov": "vitest run --coverage --dir test/unit",
    "test:watch": "vitest --dir test/unit",
    "test:e2e": "vitest run --dir test/e2e",
    "test:e2e:watch": "vitest --dir test/e2e",
    "watch": "tsc -w"
  },
  "type": "module",
  "version": "0.0.0"
}
